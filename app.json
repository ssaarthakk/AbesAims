{
  "expo": {
    "name": "AbesAims",
    "slug": "AbesAims",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/AbesAims.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/AbesAims.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/AbesAims.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.the_extremity.abes_aims"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/AbesAims.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "fffff234-d7b3-4293-bc3a-cde82f3aa8db"
      }
    }
  }
}
